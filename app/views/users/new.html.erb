<!-- < %# might need a text_field_tag for nickname, w/ default %> -->
<%= form_tag '/users', method: :post do %>
  <%= text_field_tag :name, @user.name, placeholder: "Name"%>
  <%= text_field_tag :address, @address.address, placeholder: "Address"%>
  <%= text_field_tag :city, @address.city, placeholder: "City"%>
  <%= text_field_tag :state, @address.state, placeholder: "State"%>
  <%= text_field_tag :zip, @address.zip, placeholder: "Zip"%>
  <%= text_field_tag :email, nil, placeholder: "Email"%>
  <%= password_field_tag :password, nil, placeholder: "Password"%>
  <%= password_field_tag :password_confirmation, nil, placeholder: "Confirm Password"%>
  <%= submit_tag "Submit" %>
<%end %>
